{
    "name": "docs-unlocked",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:sf": "tsc && vite build --mode salesforce && node scripts/inline-css.js && node scripts/copy-to-staticresource.js && node scripts/create-content-staticresource.js",
        "build:sf-no-manifest": "tsc && vite build --mode salesforce && node scripts/inline-css.js && node scripts/copy-to-staticresource.js && node scripts/create-content-staticresource.js --no-manifest",
        "preview": "vite preview"
    },
    "dependencies": {
        "@tailwindcss/typography": "^0.5.19",
        "dompurify": "^3.3.1",
        "highlight.js": "^11.11.1",
        "js-yaml": "^4.1.1",
        "lucide-react": "^0.363.0",
        "marked": "^17.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^7.12.0"
    },
    "devDependencies": {
        "@types/dompurify": "^3.0.5",
        "@types/js-yaml": "^4.0.9",
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@vitejs/plugin-react": "^4.2.1",
        "archiver": "^7.0.1",
        "autoprefixer": "^10.4.18",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.2.2",
        "vite": "^5.1.6"
    }
}
